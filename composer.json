{
    "name": "zajca/doctrine-encrypt-bundle",
    "type": "symfony-bundle",
    "description": "Symfony DoctrineEncryptBundle",
    "keywords": ["DBAL", "Sodium"],
    "homepage": "https://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Zaj√≠c",
            "email": "zajca@zajca.cz"
        }
    ],
    "require": {
        "php": "^7.2 || ^7.3",
        "ext-sodium": "*",
        "symfony/framework-bundle": "~3.4|~4.0",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "paragonie/hidden-string": "^1.0",
        "paragonie/ciphersweet": "^1.7.0",
        "paragonie/halite": "^4.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.0",
        "doctrine/coding-standard": "^5.0",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11"
    },
    "autoload": {
        "psr-4": { "Zajca\\Bundle\\EncryptBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Zajca\\Bundle\\EncryptBundle\\Tests\\": "" }
    },
    "extra": {
        "branch-alias": {
        }
    }
}
